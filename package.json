{
  "name": "react-debowerify-demo",
  "version": "0.0.0",
  "scripts": {
    "start": "node ./bin/www",
    "build": "browserify -t reactify -t debowerify public/js/src/app.js -o public/js/build/build.js"
  },
  "dependencies": {
    "body-parser": "~1.10.2",
    "cookie-parser": "~1.3.3",
    "debug": "~2.1.1",
    "express": "~4.11.2",
    "jade": "~1.9.1",
    "morgan": "~1.5.1",
    "serve-favicon": "~2.2.0"
  },
  "devDependencies": {
    "debowerify": "~1.2.0",
    "reactify": "~1.0.0"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  }
}
